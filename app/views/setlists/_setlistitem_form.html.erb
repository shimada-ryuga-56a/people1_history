<div class="nested-form-wrapper border p-3 mb-5 mx-2" data-new-record="<%= f.object.new_record? %>">
  <div data-controller="autocomplete" data-autocomplete-url-value="/setlists/search" role="combobox", class="mb-3">
    <%= f.text_field :song_title, class: "input input-bordered w-full min-w-xs", placeholder: "楽曲名", data: { autocomplete_target: 'input' } %>
    <ul class="list-group bg-white absolute w-full md:text-sm max-w-max" data-autocomplete-target="results"></ul>
  </div>
  <div class="mb-3 flex">
    <%= f.check_box :is_song, class: "mr-2" %>
    <%= f.label :is_song %>
    <div data-controller="dialog" data-action="click->dialog#backdropClose">
      <dialog data-dialog-target="dialog">
        <p>The modal's content here</p>
        <button type="button" data-action="dialog#close" autofocus>Cancel</button>
      </dialog>
      <button type="button" data-action="dialog#open"><i class="fa-solid fa-circle-question ml-3"></i></button>
    </div>
  </div>
  <div class="mb-3 flex">
    <%= f.check_box :is_arranged, class: "mr-2" %>
    <%= f.label :is_arranged %>
    <div data-controller="dialog" data-action="click->dialog#backdropClose">
      <dialog data-dialog-target="dialog">
        <p>The modal's content here</p>
        <button type="button" data-action="dialog#close" autofocus>Cancel</button>
      </dialog>
      <button type="button" data-action="dialog#open"><i class="fa-solid fa-circle-question ml-3"></i></button>
    </div>
  </div>
  <div class="mb-3 flex">
    <%= f.check_box :is_rehearsal, class: "mr-2" %>
    <%= f.label :is_rehearsal %>
    <div data-controller="dialog" data-action="click->dialog#backdropClose">
      <dialog data-dialog-target="dialog">
        <p>The modal's content here</p>
        <button type="button" data-action="dialog#close" autofocus>Cancel</button>
      </dialog>
      <button type="button" data-action="dialog#open"><i class="fa-solid fa-circle-question ml-3"></i></button>
    </div>
  </div>
  <div class="mb-3 flex">
    <%= f.check_box :is_encore, class: "mr-2" %>
    <%= f.label :is_encore %>
    <div data-controller="dialog" data-action="click->dialog#backdropClose">
      <dialog data-dialog-target="dialog">
        <p>The modal's content here</p>
        <button type="button" data-action="dialog#close" autofocus>Cancel</button>
      </dialog>
      <button type="button" data-action="dialog#open"><i class="fa-solid fa-circle-question ml-3"></i></button>
    </div>
  </div>

  <div>
    <button type="button" data-action="nested-form#remove" class="btn btn-active btn-ghost">この曲を削除</button>
  </div>

  <%= f.hidden_field :_destroy %>
</div>
