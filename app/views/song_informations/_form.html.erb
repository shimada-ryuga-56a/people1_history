<%= turbo_frame_tag "new_info" do %>
  <%= form_with model: info, url: song_song_informations_path(song) do |f| %>
    <div class="card shadow-2xl mb-5">
      <div class="card-body">
        <div class="flex">
          <h2 class="card-title text-base	md:text-xl">ТЃЁта▒сЂ«ТЈљСЙЏсЂФсЂћтЇћтіЏсЂіжАўсЂёсЂЌсЂЙсЂЎ№╝Ђ­ЪЎЄ</h2>
          <%= render partial: "songs/modals/song_info_modal" %>
        </div>
        <%= render 'shared/error_messages', model: f.object %>
        <%= f.text_area :body, class: "textarea", placeholder: "№╝ѕТіЋуе┐сЂ«сЃњсЃ│сЃѕ№╝Ѕ" + ["сЃАсЃ│сЃљсЃ╝сЂїУЕ▒сЂЌсЂдсЂЪсѓесЃћсѓйсЃ╝сЃЅсЂесЂІсѓњТЋЎсЂѕсЂд№╝Ђ", "сЃЕсѓцсЃќсЂ«MCсЂесЂІсЂДУЕ▒сЂЌсЂдсЂЪсѓесЃћсѓйсЃ╝сЃЅсЂѓсѓїсЂ░ТЋЎсЂѕсЂдсЂЈсЂасЂЋсЂё№╝Ђ", "сЃАсЃ│сЃљсЃ╝сЂїSNSсЂДсЂЊсЂ«ТЏ▓сЂФУДдсѓїсЂдсЂЪсѓЅТЋЎсЂѕсЂдсЂЈсЂасЂЋсЂё№╝Ђ", "сѓцсЃ│сѓ┐сЃЊсЃЦсЃ╝сЂДУЕ▒сЂЋсѓїсЂдсЂЪсѓесЃћсѓйсЃ╝сЃЅсЂфсЂЕсѓѓТЋЎсЂѕсЂдсЂЈсЂасЂЋсЂё№╝Ђ"].sample %>
        <div class="card-actions justify-end">
          <%= f.submit 'ТіЋуе┐', class: "btn btn-primary" %>
        </div>
      </div>
    </div>
  <% end %>
<% end %>